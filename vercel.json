{
  "builds": [
    {
      "src": "office_emp_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "functions": {
    "api": {
      "runtime": "python3.9",
      "maxDuration": 300,
      "memory": 512,
      "includeFiles": ["office_emp_project/**"]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/office_emp_project/wsgi.py"
    }
  ]
}

